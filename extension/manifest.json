{
    "manifest_version": 3,
    "name": "LinkEase Session Saver",
    "version": "1.0",
    "description": "Save all open tabs to LinkEase",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}